{
  "name": "@foxglove/eslint-plugin",
  "version": "2.1.0",
  "description": "Foxglove ESLint rules and configuration",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxglove/eslint-plugin.git"
  },
  "main": "index.js",
  "types": "dist/index.d.ts",
  "files": [
    "**/*.js",
    "dist"
  ],
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rm -rf dist && tsc",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "prepack": "yarn run build",
    "test": "jest"
  },
  "dependencies": {
    "@eslint/compat": "^1",
    "@eslint/js": "^9",
    "@typescript-eslint/utils": "^8",
    "eslint-config-prettier": "^10",
    "eslint-plugin-es": "^4",
    "eslint-plugin-filenames": "^1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^28",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^5",
    "tsutils": "^3",
    "typescript-eslint": "^8"
  },
  "peerDependencies": {
    "eslint": "^9.27.0"
  },
  "devDependencies": {
    "@foxglove/tsconfig": "2.0.0",
    "@swc/core": "1.7.39",
    "@swc/jest": "0.2.36",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "@typescript-eslint/rule-tester": "8.52.0",
    "eslint": "9.27.0",
    "globals": "15.11.0",
    "jest": "29.7.0",
    "prettier": "3.7.4",
    "typescript": "5.6.3"
  }
}
